    <RmGLPass NAME="RenderFire" API="OpenGL" PASS_INDEX="3" ENABLED="TRUE">
     <RmModelReference NAME="ScreenAlignedQuad" API=""/>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec2 texCoord;

uniform sampler2D fireProgress;
uniform sampler2D fireOnPaperProgress;

uniform vec4 ViewPosition;
uniform vec4 ViewDirection;
uniform vec4 UpDirection;
uniform vec4 SideVector;

uniform vec2 modelScale;
uniform float textureWidth;
uniform float offsetAngleScale;
uniform float offsetVectorScale;

uniform float time;
uniform float fireHeight;
uniform float wobbleScale;
uniform float burnSpeed;
uniform float randomnessRate;
uniform float yFade;
uniform float xFireScale;
uniform float yFireScale;

uniform sampler3D Noise;
uniform sampler2D Flame;

// this function checks if the given position is within the bounds of the "fire box", i.e. the
// box where fire could possibly be present
bool withinBounds(vec3 pos)
{
   if ( (pos.x >= -modelScale.x) && (pos.x <= modelScale.x) && (pos.z >= -modelScale.y) && (pos.z <= modelScale.y) && (pos.y >= 0.0) && (pos.y <= fireHeight) )
      return true;
   else 
      return false;
}

void main(void)
{
   // first, set the color to the original color. This might get overwritten later.
   vec4 StartColor = texture2D(fireOnPaperProgress, texCoord);

   // next, we want the "offset vector" that will point from the ViewPosition
   // towards the direction of the current fragment. A little vector magic at this point.
   float x = (texCoord.x * 2.0) - 1.0;
   float y = (texCoord.y * 2.0) - 1.0;
   vec3 offsetVector = normalize((ViewDirection.xyz + offsetAngleScale * x * SideVector.xyz + offsetAngleScale * y * UpDirection.xyz).xyz);
   
   vec3 startPosition = ViewPosition.xyz;
   
   // what we want to do here is position the "start position" at the far back.
   // This means that we will start at the first point along the line from the
   // ViewPosition, where we are outside of the paper - be it below the paper, or
   // outside the edges.
   float numY = -1.0 * (startPosition.y / offsetVector.y);   // how many offsetVectors do we need to add to get to y = 0.0?
   float numX = (sign(offsetVector.x) * modelScale.x - startPosition.x) / offsetVector.x;  // how many do we need to add to get to the x-edge?
   float numZ = (sign(offsetVector.z) * modelScale.y - startPosition.z) / offsetVector.z;
   float tmp = 0.0;
   
   if (numY <= 0.0)   // this simply means that the offsetVector is not going downwards
   {
      tmp = min(numX, numZ);
   }
   else
   {
      tmp = min(numY, numX);
      tmp = min(tmp, numZ);
   }
   
   // here, we set the start position at the edge of the paper, be it at
   // the bottom or at some far edge, whatever is closest. We add 0.1 to
   // tmp just to avoid small float imprecision.
   startPosition += (tmp + 0.1) * offsetVector;
   
   vec3 currentPosition = startPosition;
   
   bool isInside = withinBounds(currentPosition);
   
   // here, we scale the offsetVector in such a way that its length in the
   // xz-plane, is equal to the distance between two texels in the texture
   float stepSize = (length(modelScale) * 2) / textureWidth;   // this is probably not quite right
   vec2 xzVector = normalize(offsetVector.xz) * stepSize;
   float scale = offsetVector.x / xzVector.x;
   offsetVector = offsetVector / scale;
   
   vec4 fireColor = vec4(0.0);   // to this, we'll add all the fire colors along the line
   bool didBurn = false;
   float numBurnPoints = 0.0;    // the number of points along the line where we found fire
   
   for (int i = 0; i < int(textureWidth); i++)
   {
      currentPosition = startPosition - (i * offsetVectorScale * offsetVector);
         
      //are we behind the view position?
      if (sign(currentPosition.x - ViewPosition.x) != sign(offsetVector))
         break;
      
      // are we inside the fire box?   
      if (withinBounds(currentPosition))
      {
         isInside = true;
      }
      else
      {
         if (isInside)   // we went from inside the paper to outside. Might as well break!
            break;
         else
            continue;   // not time to break yet, but we want to skip to the next iteration
      }
            
      // else, read the fireProgress texture below this position!
      vec2 fireTexCoord = (currentPosition).xz;

      // bring the scale from [-texScale, texScale] to [0.0, 1.0]
      fireTexCoord = (fireTexCoord + modelScale) / (2.0 * modelScale);
      fireTexCoord.y = 1.0 - fireTexCoord.y;
      vec4 fireProgressColor = texture2D(fireProgress, fireTexCoord);   // texture written in FireProgress pass
      
      if (fireProgressColor.b > 0.5)   // fire has gone out!
         continue;
      if (fireProgressColor.g < 0.2)   // fire hasn't started!
         continue;
         
      float heightScale = fireProgressColor.w;
      float fireHeightAtThisPoint = fireHeight * heightScale;   // the maximum height of the fire at this point
      if (currentPosition.y > fireHeightAtThisPoint)
         continue;
         
      // OTHERWISE, FIRE!!
     
      // Here, we use a modified version of the Inferno-effect of the RenderMonkey Fire example.
      // the fireCoord attributes are set to a value between -1.0 and 1.0.
  
      // The shifting of the y-coordinate makes sure that the top of the fire always look right,
      // even when it's rising.
      // The x-coordinate is a mix between the x- and z-coordinates of the current position.
      // At first, they were modified to follow a /\/\/\ pattern, varying between -1.0 and 1.0. The point of
      // this was to be able to use the "same" fire pattern over and over again, and making it flow
      // smoothly all the way.
      // This would have been great if the scale of the fire was bigger (say, if it was a forest fire).
      // However, since the scale is rather small (just a paper), and especially after I was able to
      // speed up the effect, it was enough to have a very simple solution, as here. The other solution is
      // kept, because it took quite some time to derive the formula...
      vec2 fireCoord;
      //fireCoord.x = ((1.0 - (abs(currentPosition.x - 1.0 - 4.0 * floor((currentPosition.x + 1.0) / 4.0)))) + (1.0 - abs(currentPosition.z - 1.0 - 4.0 * floor((currentPosition.z + 1.0) / 4.0)))) / 2.0;
      fireCoord.x = (currentPosition.x + currentPosition.z) / (2.0 * modelScale.x);
      fireCoord.y = (currentPosition.y - (fireHeightAtThisPoint / 2.0)) / (fireHeightAtThisPoint / 2.0);
      
      // calculate fire color!
      float wobbX = 2.0 * cos(6.0 *  fireCoord.x + time);
      float wobbY = 7.0 * (1.0 - fireCoord.y) * (1.0 - fireCoord.y);
      float wobble = sin(time + wobbX + wobbY);
      
      vec3 coord;
      // Wobble more in the flames than at the base
      coord.x = xFireScale * fireCoord.x + wobbleScale * (fireCoord.y + 1.0) * wobble;
      // Create an upwards movement
      coord.y = yFireScale * fireCoord.y - burnSpeed * time;
      // Move in Z to get some randomness
      coord.z = randomnessRate * time;
      float noisy = texture3D(Noise, coord).x;
      
      // Get the color out of it all
      float heat = noisy - yFade * fireCoord.y;
      heat = clamp(heat, 0.0, 1.0);   // this gives us a sort of "inner flame" at the base, very nice!
      if (heat < 0.007)   // if we don't do this, we'll have some annoying "fog" above the fire
         continue;
      
      vec4 flame = texture2D(Flame, vec2(heat,0.0));
                  
      didBurn = true;
      numBurnPoints += 1.0;
      fireColor += flame;
   }
      
   if (didBurn)
   {
      fireColor = fireColor / numBurnPoints;
      fireColor.w = 1.0;
      gl_FragColor = mix(fireColor, StartColor, 0.1);
   }
   else
   {
      gl_FragColor = StartColor;
   }
}]]></RmShaderCode></RmGLShader>
