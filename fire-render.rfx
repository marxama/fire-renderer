     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D Texture0;
uniform sampler2D Flame;
uniform sampler2D PaperTexture;
uniform sampler2D FireLight;

varying vec2 texCoord;

void main(void)
{
   vec4 firstColor = texture2D(PaperTexture, texCoord);
   gl_FragColor = firstColor;
   vec4 Color = texture2D(Texture0, texCoord);
   float red = Color.r;
   float green = Color.g;
   float blue = Color.b;
   
   if (green < 0.2)   // no fire, just transforming the paper
   {
      float tmp = 1.0 - ((red + green) / 1.2);
      vec4 newColor = vec4(tmp, tmp, tmp, 1.0);
      gl_FragColor = mix(newColor, firstColor, tmp);
      /* this could be used in some way if we were to use the FireLight pass
      vec4 lightColor = texture2D(FireLight, texCoord);
      if (lightColor.r != 0.0)
      {  
         gl_FragColor = mix(gl_FragColor, lightColor, 0.5);
      }*/
   }
   
   
   else if (blue < 0.5)   // burning
   {
      gl_FragColor = vec4(0.0, 0.0, 0.0, 1.0);
   }
   
   else if (blue != 1.0)   // glowing ashes
   {
      // here, we make a poor attempt at coloring the glowing ashes, which are mostly white
      // close to the fire, then gradually goes towards red. It's sort of okay, but it doesn't
      // have the real glow feel to it.      
      float index = blue;
      index = 4 * index - 4 * index * index * index;
      index = clamp(index, 0.01, 0.9);
      vec4 glowColor = texture2D(Flame, vec2(index, 0.0));
      gl_FragColor = glowColor;
   }
   
   else
   {
      discard;
   }
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec2 texCoord;

uniform vec2 modelScale;

void main(void)
{
   vec4 vertex = gl_Vertex;
   vertex = vertex * vec4(modelScale.x * 2.0, 1.0, modelScale.y * 2.0, 1.0);   // scale
   gl_Position = gl_ModelViewProjectionMatrix * vertex;
   texCoord = gl_MultiTexCoord0.xy;
   texCoord.x = 1.0 - texCoord.x;
}]]></RmShaderCode></RmGLShader>
