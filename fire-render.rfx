     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D fireProgress;
uniform sampler2D Noise;
uniform sampler2D Noise2;

uniform float offset;
uniform float scaleKernel;
uniform mat3 Kernel;   // the Kernel values  tell, how much from the surrounding pixels should we add?

uniform float leftMouseButton;
uniform vec2 mouseCoords;
uniform float startFireRadius;

uniform float time;

varying vec2 texCoord;

void main(void)
{
   vec4 Color = texture2D(fireProgress, texCoord);
   float red = Color.r;
   float green = Color.g;
   float blue = Color.b;
   
   if (red == 0.0)   // the renderTexture is initialized to be green, but we want it to be black. We fix that here.
   {
      green = 0.0;
   }
   
   vec2 mCoords = mouseCoords;
   mCoords.y = 1.0 - mCoords.y;
   if (leftMouseButton == 1.0 && distance(mCoords, texCoord) <= startFireRadius)   // if leftMouseButton is pressed...
   {
      red += 0.01;
   }
   
   else   // otherwise, sample nearby texels and change our texel according to them
   {
      vec2 offsets[9];
      
      offsets[0] = vec2(-offset, -offset);
      offsets[1] = vec2(0.0, -offset);
      offsets[2] = vec2(offset, -offset);
      offsets[3] = vec2(-offset, 0.0);
      offsets[4] = vec2(0.0, 0.0);
      offsets[5] = vec2(offset, 0.0);
      offsets[6] = vec2(-offset, offset);
      offsets[7] = vec2(0.0, offset);
      offsets[8] = vec2(offset, offset);
      
      
      
      float newRed = 0.0;
      
      for (int i = 0; i < 9; i++)
      {
         float noise = texture2D(Noise, texCoord + offsets[i]).b;
         float tmp = texture2D(fireProgress, texCoord + offsets[i]).r * Kernel[i/3][i%3];
         newRed += tmp * scaleKernel * noise;
      }
      
      red += newRed / 0.2;
      red = clamp(red, 0.0, 1.0);
   }
   
   if (red == 1.0)
   {
      green += 0.02;
      green = clamp(green, 0.0, 1.0);
   }
   
   if (green == 1.0)
   {
      blue += 0.05;
      blue = clamp(blue, 0.0, 1.0);
   }
   
   // w is for scaling the height of the fire. we have some alternatives here - first, I used
   // an algorithm based on time, but actually just basing it on a texture gave a satisfying
   // result.
   //float w = abs(cos(time * texCoord.x + time2 * texCoord.y) * texture2D(Noise, texCoord).b) / 2.0 + 0.5;
   //float w = abs(cos(time * texCoord.x + time2 * texCoord.y) * texture2D(Noise, texCoord).b) * 1.75 + 0.20;   
   
   float w;
   if (green < 0.2 || blue > 0.5)
   {
      w = 0.0;
   }
   else
   {
      float heightScale = texture2D(Noise2, texCoord).r;
      heightScale = heightScale * (0.1 + heightScale) + 0.1;
      float fireDepth = ((green - 0.2) + (blue)) / 1.3;
      float heightRatio = -4.0 * fireDepth * fireDepth + 4.0 * fireDepth;
      heightRatio *= heightScale;
      w = heightRatio;
   }

   gl_FragColor = vec4(red, green, blue, w);
}]]></RmShaderCode></RmGLShader>
